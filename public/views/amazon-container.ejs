<div class='store-container white'>

  <span class="store-utility-icons">
    <!-- <i class="fa fa-refresh right"></i> -->
    <!-- <i class="fa fa-times-circle-o right"></i> -->
  </span>
  
  <h1 class="store-header">Amazon</h1>

  <div class="item" ng-repeat="item in amazonItems"> 
    <hr />
    <img ng-src="{{item.image}}" width="50px" height="50px" class="left"/>

    <div class="left">
      <h3>{{item.name}}</h3>
      <p> Stock level: {{item.stock}}</p>
    </div>

    <div class="clear"></div>
  </div>
</div>