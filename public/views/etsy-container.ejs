<div class='store-container white'>

  <span class="store-utility-icons">
    <!-- <i ng-click="updateItems(items)" class="fa fa-refresh right"></i> -->
    <!-- <i class="fa fa-times-circle-o right"></i> -->
  </span>
  
  <h1 class="store-header">Etsy</h1>

  <div class="item" ng-repeat="item in etsyItems"> 
    <hr />
    <img class="left" ng-src="http:{{item.image}}" width="50px" height="50px" />
    
    <div class="left">
      <h3>{{item.name}}</h3>
      <p> Stock level: {{item.stock}}</p>
    </div>

    <div class="clear"></div>
  </div>
</div>